{
    "name": "cedricziel/flysystem-gcs",
    "description": "Flysystem adapter for Google Cloud Storage (GCS) that uses gcloud-php",
    "keywords": [
        "flysystem",
        "cloud",
        "filesystem",
        "google",
        "gcs",
        "google cloud storage"
    ],
    "type": "library",
    "require": {
        "php": "^7.2 || ^8.0",
        "google/cloud-storage": "^1.3",
        "league/flysystem": "^2.0",
        "symfony/polyfill-php80": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.4",
        "league/flysystem-adapter-test-utilities": "^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Cedric Ziel",
            "email": "cedric@cedric-ziel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CedricZiel\\FlysystemGcs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CedricZiel\\FlysystemGcs\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
